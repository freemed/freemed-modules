<freemedpackage>
	<information>
		<name>Food Assistance</name>
		<author>Jeff Buchbinder</author>
		<version>0.1</version>
		<description>FQHC emergency food assistance program support modules.</description>
		<minimumversion>0.9.0</minimumversion>
	</information>
	<install>
		<directory>
			<destination>modules/food_assistance/</destination>
		</directory>
	<!-- ----- Modules ----- -->
		<file>
			<source>food_assistance_person.db.module.php</source>
			<destination>modules/food_assistance/</destination>
		</file>
		<file>
			<source>food_assistance_dispense.db.module.php</source>
			<destination>modules/food_assistance/</destination>
		</file>
		<file>
			<source>food_assistance_item.db.module.php</source>
			<destination>modules/food_assistance/</destination>
		</file>
		<file>
			<source>food_assistance_inventory.db.module.php</source>
			<destination>modules/food_assistance/</destination>
		</file>
	<!-- ----- MySQL Schema Files ----- -->
		<file>
			<source>foodassistanceperson.sql</source>
			<destination>data/schema/mysql/</destination>
		</file>
		<file>
			<source>foodassistancedispense.sql</source>
			<destination>data/schema/mysql/</destination>
		</file>
		<file>
			<source>foodassistanceinventory.sql</source>
			<destination>data/schema/mysql/</destination>
		</file>
		<file>
			<source>foodassistanceitem.sql</source>
			<destination>data/schema/mysql/</destination>
		</file>
	<!-- ----- User Interface ----- -->
	</install>
</freemedpackage>
